{
  "name": "cdk-spa-deploy",
  "version": "1.47.0",
  "description": "This is an AWS CDK Construct to make deploying a single page website (Angular/React/Vue) to AWS S3 behind SSL/Cloudfront as easy as 5 lines of code.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "test": "tsc && jest",
    "cdk": "cdk",
    "package": "jsii-pacmak"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-spa-deploy",
        "module": "spa_deploy"
      }
    }
  },
  "awscdkio": {
    "twitter": "nideveloper"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.47.0",
    "@types/jest": "^26.0.3",
    "@types/node": "14.0.14",
    "aws-cdk": "1.47.0",
    "jest": "^26.1.0",
    "jsii": "1.7.0",
    "jsii-pacmak": "1.7.0",
    "ts-jest": "^26.1.1",
    "jest-extended": "^0.11.5",
    "jest-junit": "^11.0.1",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.5",
    "source-map-support": "^0.5.19"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "1.47.0",
    "@aws-cdk/aws-cloudfront": "1.47.0",
    "@aws-cdk/aws-route53-patterns": "1.47.0",
    "@aws-cdk/aws-route53-targets": "1.47.0",
    "@aws-cdk/aws-s3": "1.47.0",
    "@aws-cdk/aws-s3-deployment": "1.47.0",
    "@aws-cdk/core": "1.47.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudfront": "1.47.0",
    "@aws-cdk/aws-s3": "1.47.0",
    "@aws-cdk/aws-s3-deployment": "1.47.0",
    "@aws-cdk/aws-certificatemanager": "1.47.0",
    "@aws-cdk/core": "1.47.0",
    "@aws-cdk/aws-route53-patterns": "1.47.0",
    "@aws-cdk/aws-route53-targets": "1.47.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "spa",
    "website",
    "deploy",
    "cloudfront"
  ],
  "author": "hello@cdkpatterns.com",
  "repository": {
    "url": "https://github.com/nideveloper/CDK-SPA-Deploy.git",
    "type": "git"
  },
  "license": "MIT"
}
